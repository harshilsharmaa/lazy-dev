{
  "name": "lazy-geek",
  "version": "2.0.0",
  "description": "A library to set up your basic app requirement and make all files and folder required for nodejs development.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "postinstall": "node node_modules/lazy-geek/lib/index.js",
    "start": "lib/index.js"
  },
  "bin": {
    "create-app": "src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/harshilsharmaa/lazy-geek.git"
  },
  "keywords": [
    "lazy-dev",
    "folder-structure",
    "basic-app",
    "MERN folder structure",
    "nodejs folder structure"
  ],
  "author": "Harshil Sharma",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/harshilsharmaa/lazy-geek/issues"
  },
  "homepage": "https://github.com/harshilsharmaa/lazy-geek#readme",
  "devDependencies": {
    "@types/node": "^18.7.18",
    "typescript": "^4.8.3"
  },
  "files": [
    "lib/**/*"
  ],
  "directories": {
    "lib": "lib"
  },
  "dependencies": {
    "lazy-geek": "^1.1.0"
  }
}
